
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <h1>This is List View</h1>
    <table>
        <thead>
            <tr>
                <th>번호</th>
                <th>제목</th>
                <th>글쓴이</th>
            </tr>
        </thead>
        <tbody>
            @for (int i=1; i<=10; i++) {
                <tr>
                    <td>@i</td>
                    <td>제목입니다.</td>
                    <td>2sjin</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
